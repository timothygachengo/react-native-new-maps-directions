{
  "name": "react-native-new-maps-directions",
  "version": "1.0.0",
  "description": "TypeScript React Native component for rendering Google Maps directions and routes as polylines. Supports driving, walking, cycling, transit, and two-wheeler travel modes with full TypeScript support and automatic route fetching.",
  "contributors": [],
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-new-maps-directions-example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "version": "changeset version",
    "publish": "changeset publish --git-tag"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "maps",
    "react-native-maps",
    "react-native-maps-directions",
    "react-native-new-maps-directions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timothygachengo/react-native-new-maps-directions.git"
  },
  "author": "Timothy Mugo Gachengo <timothy@timothygachengo.com> (https://timothygachengo.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timothygachengo/react-native-new-maps-directions/issues"
  },
  "homepage": "https://github.com/timothygachengo/react-native-new-maps-directions#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@evilmartians/lefthook": "^1.12.3",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "^0.81.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@svitejs/changesets-changelog-github-compact": "^1.2.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.12",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-builder-bob": "^0.40.13",
    "react-native-maps": "^1.26.18",
    "react-test-renderer": "19.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-maps": "*"
  },
  "workspaces": [
    "example",
    "."
  ],
  "packageManager": "yarn@3.6.1",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "js",
    "type": "library",
    "version": "0.54.8"
  }
}
